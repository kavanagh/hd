{
  "name": "FT Reactions Hackday",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Thing for demoing on the FT hackday",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ft.com/*"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "*://*.ft.com/*"
  ]
}
